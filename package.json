{
  "name": "github-code-review-bot",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf dist && ncc build src/index.cjs --target es2021",
    "start": "probot run ./dist/index.js",
    "test": "vitest",
    "format": "prettier --check '*.{js,md,json}' '.github/**/*.yml'",
    "format:fix": "prettier --write '*.{js,md,json}' '.github/**/*.yml'"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@probot/adapter-github-actions": "^4.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.7",
    "probot": "^13.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@vercel/ncc": "^0.38.1",
    "nock": "^14.0.0-beta.8",
    "prettier": "^3.3.3",
    "vitest": "^2.0.3",
    "typescript": "^5.5.3"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js",
      "lib/**"
    ]
  },
  "engines": {
    "node": ">= 20"
  },
  "packageManager": "pnpm@9.4.0+sha1.9217c800d4ab947a7aee520242a7b70d64fc7638"
}
